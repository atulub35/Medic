<div>
    
    <%= turbo_frame_tag dom_id(temperature) do %>
        <div class="">
              <%= temperature.title %>
              <!-- <%= range_field_tag 'title', temperature.title, :in => 0..100, :step => '.1' %> -->
              <div class="progress">
                <div class="determinate" style="width: <%= temperature.title %>%"></div>
            </div>
        </div>
    <% end %>
    <% if @procedure.present? %>
        <%= turbo_frame_tag dom_id(temperature), src: edit_procedure_temperature_path(@procedure, temperature), target: :_top %>
    <% end %>
    <% if @procedure.present? %>
        <%= link_to 'Go To Edit View', edit_procedure_temperature_path(@procedure, temperature), class: 'btn m-l-5' %>
    <% end %>
</div>
